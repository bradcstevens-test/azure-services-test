{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworks_msftlearn_vnet1_name": {
            "defaultValue": "msftlearn-vnet1",
            "type": "String"
        },
        "virtualNetworks_msftlearn_vnet2_name": {
            "defaultValue": "msftlearn-vnet2",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-09-01",
            "name": "[parameters('virtualNetworks_msftlearn_vnet1_name')]",
            "location": "centralus",
            "tags": {
                "Department": "Finance",
                "Environment": "Training"
            },
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "192.168.0.0/24"
                    ]
                },
                "subnets": [
                    {
                        "name": "Subnet1",
                        "properties": {
                            "addressPrefix": "192.168.0.0/24",
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-09-01",
            "name": "[parameters('virtualNetworks_msftlearn_vnet2_name')]",
            "location": "centralus",
            "tags": {
                "Department": "Marketing ",
                "Environment": "Training"
            },
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "192.168.100.0/24"
                    ]
                },
                "subnets": [
                    {
                        "name": "Subnet2",
                        "properties": {
                            "addressPrefix": "192.168.100.0/24",
                            "delegations": [],
                            "privateEndpointNetworkPolicies": "Enabled",
                            "privateLinkServiceNetworkPolicies": "Enabled"
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('virtualNetworks_msftlearn_vnet1_name'), '/Subnet1')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_msftlearn_vnet1_name'))]"
            ],
            "properties": {
                "addressPrefix": "192.168.0.0/24",
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        },
        {
            "type": "Microsoft.Network/virtualNetworks/subnets",
            "apiVersion": "2019-09-01",
            "name": "[concat(parameters('virtualNetworks_msftlearn_vnet2_name'), '/Subnet2')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/virtualNetworks', parameters('virtualNetworks_msftlearn_vnet2_name'))]"
            ],
            "properties": {
                "addressPrefix": "192.168.100.0/24",
                "delegations": [],
                "privateEndpointNetworkPolicies": "Enabled",
                "privateLinkServiceNetworkPolicies": "Enabled"
            }
        }
    ]
}