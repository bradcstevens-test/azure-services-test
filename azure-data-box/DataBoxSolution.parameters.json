{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "RegionName"
        },
        "storageAccountName": {
            "value": "storageaccountname"
        },
        "accountType": {
            "value": "[Standard_LRS,Standard_ZRS,Standard_GRS]"
        },
        "kind": {
            "value": "StorageV2"
        },
        "accessTier": {
            "value": "[Hot,Cold,Archive]"
        },
        "supportsHttpsTrafficOnly": {
            "value": true
        }
    }
}